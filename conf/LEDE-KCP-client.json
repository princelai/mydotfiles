{
  "log": {
    "loglevel": "warning",
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log"
  },
  "inbound": {
    "port": 1080,
    "protocol": "socks",
    "domainOverride": ["tls", "http"],
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    },
    "settings": {
      "auth": "noauth",
      "udp": true
    }
  },
  "inboundDetour": [
    {
      "domainOverride": ["tls", "http"],
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "port": 1060,
      "protocol": "dokodemo-door",
      "settings": {
        "network": "tcp,udp",
        "followRedirect": true
      }
    }
  ],
  "outbound": {
    "protocol": "vmess",
    "settings": {
      "vnext": [
        {
          "address": "solarck.top",
          "port": 8082,
          "users": [
            {
              "id": "998c1fa3-962a-4682-a56d-ad262f7f184e",
              "alterId": 32,
              "security": "auto",
              "level": 0
            }
          ]
        },
        {
          "address": "lunarch.top",
          "port": 8082,
          "users": [
            {
              "id": "998c1fa3-962a-4682-a56d-ad262f7f184e",
              "alterId": 32,
              "security": "auto",
              "level": 0
            }
          ]
        },
        {
          "address": "chenwrt.top",
          "port": 8082,
          "users": [
            {
              "id": "998c1fa3-962a-4682-a56d-ad262f7f184e",
              "alterId": 32,
              "security": "auto",
              "level": 0
            }
          ]
        }
      ]
    },
    "streamSettings": {
      "network": "mkcp",
      "kcpSettings": {
        "mtu": 1350,
        "tti": 20,
        "uplinkCapacity": 20,
        "downlinkCapacity": 50,
        "congestion": false,
        "readBufferSize": 2,
        "writeBufferSize": 2,
        "header": {
          "type": "dtls"
        }
      }
    },
    "mux": {
      "enabled": true,
      "concurrency": 8
    }
  },
  "outboundDetour": [
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "block"
    }
  ],
  "dns": {
    "servers": ["8.8.8.8", "8.8.4.4", "localhost"]
  },
  "routing": {
    "strategy": "rules",
    "settings": {
      "domainStrategy": "IPIfNonMatch",
      "rules": [
        {
          "type": "field",
          "ip": ["geoip:cn", "geoip:private", "172.168.0.0/16"],
          "port": "0-1024",
          "network": "tcp,udp",
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "domain": ["geosite:cn"],
          "port": "0-1024",
          "network": "tcp,udp",
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "outboundTag": "direct",
          "network": "tcp,udp",
          "domain": ["ext:h2y.dat:top500_direct"]
        },
        {
          "type": "field",
          "outboundTag": "block",
          "network": "tcp,udp",
          "domain": ["ext:h2y.dat:mad", "ext:h2y.dat:ad"]
        }
      ]
    }
  },
  "policy": {
    "levels": {
      "0": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "statsUserUplink": false,
        "statsUserDownlink": false,
        "bufferSize": 1024
      }
    },
    "system": {
      "statsInboundUplink": false,
      "statsInboundDownlink": false
    }
  }
}
